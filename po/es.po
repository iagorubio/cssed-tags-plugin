#: src/tagplugin.c:39
msgid "Tagplugin"
msgstr "Tagplugin"

#. the plugin name
#: src/tagplugin.c:40
msgid "Frontend plugin for ctags"
msgstr "Plugin con soporte para ctags"

#: src/tagplugin.c:67 src/interface.c:433
msgid "Tags"
msgstr "Tags"

#: src/interface.c:88 src/interface.c:244
msgid "Create tag file"
msgstr "Crear archivo tag"

#: src/interface.c:92
msgid "Open tag file"
msgstr "Abrir archivo tag"

#: src/interface.c:96
msgid "Current tag file"
msgstr "Archivo actual"

#: src/interface.c:104
msgid "This is the tag file in use if any"
msgstr "Este es el archivo en use, si hay alguno"

#: src/interface.c:114
msgid "Enter tag name"
msgstr "Introduce nombre a buscar"

#: src/interface.c:116
msgid "Partial match"
msgstr "Coincidendia parcial"

#: src/interface.c:119
msgid "Performs a partial match search"
msgstr "Realiza una busqueda con coincidencia parcial"

#: src/interface.c:121
msgid "Case sensitive"
msgstr "Caso sensitivo"

#: src/interface.c:127
msgid "Case insensitive"
msgstr "Caso insensitivo"

#: src/interface.c:136
msgid "Click to search tag"
msgstr "Pulsar para buscar la etiqueta"

#: src/interface.c:138
msgid "Show all"
msgstr "Mostrar todas"

#: src/interface.c:152
msgid "Tag"
msgstr "Tags"

#: src/interface.c:158
msgid "File"
msgstr "Archivo"

#: src/interface.c:163
msgid "Line"
msgstr "Línea"

#: src/interface.c:258
msgid "Base directory"
msgstr "Directorio base"

#: src/interface.c:265
msgid "Enter here the base directory to scan for tags"
msgstr "Introduzca aquí el directorio base para la busqueda"

#: src/interface.c:277
msgid "Opens a directory to be used as the base directory."
msgstr "Abre un directorio para usar como directorio base"

#: src/interface.c:283
msgid "Recursive"
msgstr "Recursiva"

#: src/interface.c:286
msgid "Descend recursively all directories"
msgstr "Bajar recursivamente todos los directorios"

#: src/interface.c:289
msgid "Force language"
msgstr "Forzar lenguaje"

#: src/interface.c:292
msgid "Force file to be treated as a selected language"
msgstr "Forzar los archivos a ser tratados como de un lenguaje determinado"

#: src/interface.c:337
msgid "In case of forcing a language it will be taken from here"
msgstr "En caso de forzar el lenguaje, este será tomado de aquí"

#: src/interface.c:340
msgid "Exclude patterns"
msgstr "Excluir patrones"

#: src/interface.c:347
msgid "Enter patterns to exclude from the search"
msgstr "Introduzcir patrones de nombre e archivo, para excluir (P.Ej:*.c)"

#: src/interface.c:353
msgid "Change language map "
msgstr "Cambiar el mapa de lenguajes"

#: src/interface.c:356
msgid ""
"Changes the language map: with \"PHP:.lib\" only *.lib files are recognized "
"as PHP, with \"PHP:+.lib\" *.lib is added to the PHP recognized patterns."
msgstr ""
"Cambia el mapa de lenguajes: con \"PHP:.lib\" solo los archivos *.lib son "
"reconocidoscomo PHP, con \"PHP:+.lib\" el patrón *.lib es añadido a los "
"patrones reconocidos como PHP."

#: src/interface.c:361
msgid "Enter here the ctags language map (see man ctags for more info)"
msgstr "Introduce aquí el mapa de lenguajes para ctags (ver man ctags)"

#: src/interface.c:370
msgid "Command output"
msgstr "Salida del comando"

#: src/interface.c:440
msgid "Create/Update tag file in this file's directory"
msgstr "Crear/Actualizar archivo de tags en ese directorio"

#: src/interface.c:444
msgid "Find this tag in current tag file"
msgstr "Buscar este tag en archivo de tags actual"

#: src/interface.c:449
msgid "Go to this tag in current tag file"
msgstr "Ir a este tag"

#: src/callbacks.c:67
msgid "Select tag file to open"
msgstr "Selecciona archivo de tags para abrir"

#: src/callbacks.c:95 src/callbacks.c:149 src/callbacks.c:498
msgid "No open tag file"
msgstr "No hay archivo de tags abierto"

#: src/callbacks.c:95 src/callbacks.c:149 src/callbacks.c:498
msgid "First open or create a tag file to search"
msgstr "Antes abra o cree el archivo de tags"

#: src/callbacks.c:102 src/callbacks.c:156 src/callbacks.c:505
msgid "Error opening tag file"
msgstr "Error abriendo el archivo"

#: src/callbacks.c:102 src/callbacks.c:156 src/callbacks.c:505
#, c-format
msgid ""
"Cannot open tag file\n"
"%s\n"
"\n"
"Error: %s"
msgstr ""
"No puedo abrir el archivo\n"
"%s\n"
"\n"
"Error: %s"

#: src/callbacks.c:235
msgid "Select a directory"
msgstr "Selecciona un directorio"

#: src/callbacks.c:317 src/callbacks.c:468
msgid "No base directory set"
msgstr "Aun no se seleccionado directorio base"

#: src/callbacks.c:317
msgid "You must set a base directory first"
msgstr "Debes de seleccionar un directorio base"

#: src/callbacks.c:468
msgid ""
"The file has not been saved, hence I cannot know the base directory.\n"
"Save the file and try it again"
msgstr ""
"El archivo no está guardado y no conozco el directorio base.\n"
"Guarde elarchivo e inténtelo de nuevo."

